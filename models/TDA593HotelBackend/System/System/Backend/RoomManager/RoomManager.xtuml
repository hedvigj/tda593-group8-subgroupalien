INSERT INTO O_TFR
	VALUES ("aceb78d7-9a1f-46e7-a99a-85fcba249fdc",
	"b1404731-4bdc-477f-ab49-48cf034fe57f",
	'getRoomCount',
	'',
	"5f77c811-d46b-4197-a574-4b3505e92410",
	1,
	'select many rooms from instances of RoomInstance;

assign count = 0;
if(not_empty rooms)
	for each room in rooms
